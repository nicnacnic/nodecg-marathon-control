  "dependencies": {
    "@discordjs/opus": "^0.5.3",
    "@discordjs/voice": "^0.7.5",
    "audio-mixer": "^2.1.4",
    "discord.js": "^13.3.1",
    "get-current-line": "^6.6.0",
    "naudiodon": "^2.3.6",
    "obs-websocket-js": "4.0.3",
    "prism-media": "^1.3.2"
  }
}
