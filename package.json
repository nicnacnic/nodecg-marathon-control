{
  "name": "nodecg-marathon-control",
  "version": "1.1.1",
  "description": "A NodeCG bundle to control all aspects of a marathon's stream.",
  "homepage": "https://github.com/nicnacnic/nodecg-marathon-control",
  "license": "MIT",
  "author": "nicnacnic",
  "nodecg": {
    "compatibleRange": "^1.8.1",
    "bundleDependencies": {
      "nodecg-speedcontrol": "^2.4.0"
    },
    "assetCategories": [
      {
        "name": "camCover",
        "title": "Cam Cover",
        "allowedTypes": [
          "jpg",
          "jpeg",
          "gif",
          "png"
        ]
      }
    ],
    "dashboardPanels": [
      {
        "name": "previewProgram",
        "title": "Preview/Program",
        "width": 9,
        "file": "previewProgram/previewProgram.html"
      },
      {
        "name": "audio",
        "title": "Audio",
        "width": 3,
        "file": "audio/audio.html"
      },
      {
        "name": "sceneControl",
        "title": "Scene Control",
        "width": 3,
        "file": "sceneControl/sceneControl.html"
      },
      {
        "name": "streamSync",
        "title": "StreamSyncâ„¢",
        "width": 3,
        "file": "streamSync/streamSync.html"
      },
      {
        "name": "stats",
        "title": "Stats",
        "width": 3,
        "file": "stats/stats.html"
      },
      {
        "name": "checklist",
        "title": "Checklist",
        "width": 3,
        "file": "checklist/checklist.html"
      },
      {
        "name": "obsSettings",
        "title": "General",
        "width": 3,
        "file": "obsSettings/obsSettings.html",
        "workspace": "Settings"
      },
      {
        "name": "autoRecordSettings",
        "title": "Auto Record",
        "width": 3,
        "file": "autoRecordSettings/autoRecordSettings.html",
        "workspace": "Settings"
      },
      {
        "name": "streamSyncSettings",
        "title": "Stream Sync",
        "width": 3,
        "file": "streamSyncSettings/streamSyncSettings.html",
        "workspace": "Settings"
      },
      {
        "name": "adPlayerSettings",
        "title": "Ad Player",
        "width": 3,
        "file": "adPlayerSettings/adPlayerSettings.html",
        "workspace": "Settings"
      },
      {
        "name": "notice",
        "title": "",
        "width": 4,
        "dialog": true,
        "dialogButtons": [
          {
            "name": "Ok",
            "type": "confirm"
          }
        ],
        "file": "notice/notice.html"
      },
      {
        "name": "welcome",
        "title": "",
        "width": 5,
        "dialog": true,
        "dialogButtons": [
          {
            "name": "Ok",
            "type": "confirm"
          }
        ],
        "file": "welcome/welcome.html"
      }
    ],
    "graphics": [
      {
        "file": "delay/delay.html",
        "width": 1,
        "height": 1
      },
      {
        "file": "streamPlayer/streamPlayer1.html",
        "width": 1920,
        "height": 1080,
        "singleInstance": true
      },
      {
        "file": "streamPlayer/streamPlayer2.html",
        "width": 1920,
        "height": 1080,
        "singleInstance": true
      },
      {
        "file": "streamPlayer/streamPlayer3.html",
        "width": 1920,
        "height": 1080,
        "singleInstance": true
      },
      {
        "file": "streamPlayer/streamPlayer4.html",
        "width": 1920,
        "height": 1080,
        "singleInstance": true
      },
      {
        "file": "camCover/camCover1.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "camCover/camCover2.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "camCover/camCover3.html",
        "width": 1920,
